<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="mod/quiz/accessrule/oqylyq/db" VERSION="2019120400" COMMENT="XMLDB file for Moodle mod/quiz/accessrule/oqylyq"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:noNamespaceSchemaLocation="../../../../../lib/xmldb/xmldb.xsd">
    <TABLES>
        <TABLE NAME="quizaccess_oql_quizsettings" COMMENT="Stores the quiz level Oqylyq configuration.">
            <FIELDS>
                <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
