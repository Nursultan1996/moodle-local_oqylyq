                <FIELD NAME="cmid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" COMMENT="Foreign key to course module id."/>
                <FIELD NAME="proctoring" TYPE="int" LENGTH="1" NOTNULL="false" SEQUENCE="false" COMMENT=""/>
                <FIELD NAME="application" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" COMMENT=""/>
                <FIELD NAME="main_camera_record" TYPE="int" LENGTH="1" NOTNULL="false" SEQUENCE="false" COMMENT=""/>
                <FIELD NAME="second_camera_record" TYPE="int" LENGTH="1" NOTNULL="false" SEQUENCE="false" COMMENT=""/>
                <FIELD NAME="screen_share_record" TYPE="int" LENGTH="1" NOTNULL="false" SEQUENCE="false" COMMENT=""/>
                <FIELD NAME="photo_head_identity" TYPE="int" LENGTH="1" NOTNULL="false" SEQUENCE="false" COMMENT=""/>
                <FIELD NAME="id_verification" TYPE="int" LENGTH="1" NOTNULL="false" SEQUENCE="false" COMMENT=""/>
                <FIELD NAME="display_checks" TYPE="int" LENGTH="1" NOTNULL="false" SEQUENCE="false" COMMENT=""/>
                <FIELD NAME="hdcp_checks" TYPE="int" LENGTH="1" NOTNULL="false" SEQUENCE="false" COMMENT=""/>
